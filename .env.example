# Supabase
SUPABASE_URL=your_supabase_url
SUPABASE_ANON_KEY=your_supabase_anon_key

# Deep Link Configuration (for password reset and email verification)
# iOS/Android: apptemplate://auth-callback
# Web (local): http://localhost:port/auth-callback (configured in Supabase dashboard)
# Web (production): https://yourdomain.com/auth-callback
SUPABASE_REDIRECT_URL=apptemplate://auth-callback

# RevenueCat Configuration
# Get your API key from: https://app.revenuecat.com/settings/api-keys
# Use the public SDK key (starts with "appl_" for iOS or "goog_" for Android)
# Platform Support: iOS, Android, macOS only (Web falls back to free tier automatically)
REVENUECAT_API_KEY=your_revenuecat_api_key

# RevenueCat Entitlement Identifier (default: "premium")
# Configure entitlements in RevenueCat Dashboard â†’ Entitlements
# This identifier is used to check if the user has an active subscription
# The app checks: customerInfo.entitlements.active["premium"]?.isActive
# Make sure this matches your RevenueCat dashboard configuration
REVENUECAT_ENTITLEMENT_ID=premium

# Sentry
SENTRY_DSN=your_sentry_dsn
